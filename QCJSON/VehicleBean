package JSON;

import java.io.Serializable;
import java.util.HashMap;

public class VehicleBean implements Serializable{
    private String make;
    private String model;
    private Integer year;
    private String color;

    public VehicleBean(){

    }

    public VehicleBean(String make, String model, Integer year, String color) {
        this.make = make;
        this.model = model;
        this.year = year;
        this.color = color;
    }


}
